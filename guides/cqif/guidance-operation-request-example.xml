<?xml version="1.0" encoding="UTF-8"?>
<Parameters xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://hl7.org/fhir ../../schema/fhir-single.xsd">
  <!-- This example demonstrates the request for a guidance operation using the GuidanceRequest proposed resource. -->
  <parameter>
    <name value="request"/>
    <resource>
		<GuidanceRequest>
			<initiatingOrganization>
				<reference value="Organization/1"/>
			</initiatingOrganization>
			<initiatingPerson>
				<reference value="Practitioner/1"/>
			</initiatingPerson>
			<systemUserType>
				<coding>
					<system value="TBD"/>
					<code value="provider"/> 
				</coding>
			</systemUserType>
			<systemUserLanguage>
				<coding>
					<system value=""/>
					<code value=""/>
				</coding>
			</systemUserLanguage>
			<receivingOrganization>
				<reference value="Organization/1"/>
			</receivingOrganization>
			<recipientType>
				<coding>
					<system value="TBD"/>
					<code value="provider"/>
				</coding>
			</recipientType>
			<recipientLanguage>
				<system value=""/>
				<code value=""/>
