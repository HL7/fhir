		  </telecom>
		  <telecom>
			<system value="phone"/>
			<value value="(03) 5555 6473"/>
			<use value="work"/>
		  </telecom>
		
		  <!-- use FHIR code system for male / female -->
		  <gender value="male" />
		  <birthDate value="1974-12-25"/>
		  <deceasedBoolean value="false"/>
		
		  <address>
			<use value="home"/>
			<line value="534 Erewhon St"/>
			<city value="PleasantVille"/>
			<state value="Vic"/>
			<postalCode value="3999"/>
