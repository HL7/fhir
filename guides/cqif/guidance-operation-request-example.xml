			<encounterType>
				<coding>
					<system value="http://hl7.org/fhir/encounter-class"/>
					<code value="inpatient"/>
				</coding>
			</encounterType>
		</GuidanceRequest>
    </resource>
  </parameter> 
  <parameter>
    <name value="resource"/>
    <resource>
		<Patient>
		  <id value="example"/>
		  <meta>
			  <profile value="http://hl7.org/fhir/StructureDefinition/patient-qicore-qicore-patient"/>
