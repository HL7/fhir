      "code" : "171WH0202X",
      "display" : "Home Modifications"
    },
    {
      "extension" : [{
        "url" : "http://hl7.org/fhir/StructureDefinition/valueset-definition",
        "valueString" : "A contractor who makes modifications to private vehicles to accommodate a health condition."
      }],
      "system" : "http://nucc.org/provider-taxonomy",
      "code" : "171WV0202X",
      "display" : "Vehicle Modifications"
    },
    {
