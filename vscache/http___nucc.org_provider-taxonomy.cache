      "display" : "Periodontist",
      "designation" : [{
        "value" : "Periodontist"
      }]
    },
    {
      "extension" : [{
        "url" : "http://hl7.org/fhir/StructureDefinition/valueset-definition",
        "valueString" : "That branch of dentistry pertaining to the restoration and maintenance of oral functions, comfort, appearance and health of the patient by the restoration of natural teeth and/or the replacement of missing teeth and contiguous oral and maxillofacial tissues with artificial substitutes."
      }],
      "system" : "http://nucc.org/provider-taxonomy",
      "code" : "1223P0700X",
