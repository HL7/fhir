        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="SpecimenDefinition.typeTested.type"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/SpecimenDefinition-type-tested"/>
        <description value="The type of specimen conditioned for testing"/>
        <code value="type-tested"/>
        <type value="token"/>
        <expression value="SpecimenDefinition.typeTested.type"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="SpecimenDefinition-url"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="normative"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="SpecimenDefinition.url"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/SpecimenDefinition-url"/>
        <description value="The uri that identifies the specimen definition"/>
        <code value="url"/>
        <type value="uri"/>
