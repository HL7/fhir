<?xml version="1.0" encoding="UTF-8"?>

<Bundle xmlns="http://hl7.org/fhir">
  <id value="SpecimenDefinition-search-params"/>
  <entry>
    <resource>
      <SearchParameter>
        <id value="SpecimenDefinition-container"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="SpecimenDefinition.typeTested.container.type"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/SpecimenDefinition-container"/>
        <description value="The type of specimen conditioned in container expected by the lab"/>
        <code value="container"/>
        <type value="token"/>
        <expression value="SpecimenDefinition.typeTested.container.type"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="SpecimenDefinition-experimental"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="normative"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="SpecimenDefinition.experimental"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/SpecimenDefinition-experimental"/>
        <description value="Not for genuine usage (true)"/>
        <code value="experimental"/>
        <type value="token"/>
        <expression value="SpecimenDefinition.experimental"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="SpecimenDefinition-identifier"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="SpecimenDefinition.identifier"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/SpecimenDefinition-identifier"/>
        <description value="The unique identifier associated with the SpecimenDefinition"/>
        <code value="identifier"/>
        <type value="token"/>
        <expression value="SpecimenDefinition.identifier"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="SpecimenDefinition-is-derived"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="normative"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="SpecimenDefinition.typeTested.isDerived"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/SpecimenDefinition-is-derived"/>
        <description value="Primary specimen (false) or derived specimen (true)"/>
        <code value="is-derived"/>
        <type value="token"/>
        <expression value="SpecimenDefinition.typeTested.isDerived"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="SpecimenDefinition-status"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="normative"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="SpecimenDefinition.status"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/SpecimenDefinition-status"/>
        <description value="Publication status of the SpecimenDefinition: draft, active, retired, unknown"/>
        <code value="status"/>
        <type value="token"/>
        <expression value="SpecimenDefinition.status"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="SpecimenDefinition-title"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
