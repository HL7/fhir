        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="QuestionnaireResponse.basedOn"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/QuestionnaireResponse-based-on"/>
        <description value="Plan/proposal/order fulfilled by this questionnaire response"/>
        <code value="based-on"/>
        <type value="reference"/>
        <expression value="QuestionnaireResponse.basedOn"/>
        <!--<xpath value="f:QuestionnaireResponse/f:basedOn"/> -->
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="QuestionnaireResponse-encounter"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="QuestionnaireResponse.encounter"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/QuestionnaireResponse-encounter"/>
        <description value="Encounter associated with the questionnaire response"/>
        <code value="encounter"/>
        <type value="reference"/>
        <expression value="QuestionnaireResponse.encounter"/>
        <!--<xpath value="f:QuestionnaireResponse/f:encounter"/> -->
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="QuestionnaireResponse-identifier"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="QuestionnaireResponse.identifier"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/QuestionnaireResponse-identifier"/>
        <description value="The unique identifier for the questionnaire response"/>
        <code value="identifier"/>
        <type value="token"/>
        <expression value="QuestionnaireResponse.identifier"/>
        <!--<xpath value="f:QuestionnaireResponse/f:identifier"/> -->
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="QuestionnaireResponse-part-of"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="QuestionnaireResponse.partOf"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/QuestionnaireResponse-part-of"/>
        <description value="Procedure or observation this questionnaire response was performed as a part of"/>
        <code value="part-of"/>
        <type value="reference"/>
        <expression value="QuestionnaireResponse.partOf"/>
        <!--<xpath value="f:QuestionnaireResponse/f:partOf"/> -->
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="QuestionnaireResponse-patient"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="QuestionnaireResponse.subject"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/QuestionnaireResponse-patient"/>
        <description value="The patient that is the subject of the questionnaire response"/>
        <code value="patient"/>
        <type value="reference"/>
        <expression value="QuestionnaireResponse.subject.where(resolve() is Patient)"/>
        <!--<xpath value="f:QuestionnaireResponse/f:subject"/> -->
        <processingMode value="normal"/>
        <target value="Patient"/>
      </SearchParameter>
    </resource>
  </entry>
