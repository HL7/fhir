    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="QuestionnaireResponse-item-subject"/>
        <url value="http://hl7.org/fhir/SearchParameter/QuestionnaireResponse-item-subject"/>
        <version value="5.0.0"/>
        <name value="item-subject"/>
        <status value="draft"/>
        <experimental value="true"/>
        <description value="Allows searching for QuestionnaireResponses by item value where the item has isSubject=true"/>
        <code value="item-subject"/>
        <base value="QuestionnaireResponse"/>
        <type value="reference"/>
        <expression value="QuestionnaireResponse.item.where(extension('http://hl7.org/fhir/StructureDefinition/questionnaireresponse-isSubject').exists()).answer.value.ofType(Reference)"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
</Bundle>