  <entry>
    <resource>
      <SearchParameter>
        <id value="QuestionnaireResponse-subject"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="QuestionnaireResponse.subject"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/QuestionnaireResponse-subject"/>
        <description value="The subject of the questionnaire response"/>
        <code value="subject"/>
        <type value="reference"/>
        <expression value="QuestionnaireResponse.subject"/>
        <!--<xpath value="f:QuestionnaireResponse/f:subject"/> -->
        <processingMode value="normal"/>
