        <code value="udi-carrier"/>
        <type value="string"/>
        <expression value="Device.udiCarrier.carrierHRF"/>
        <xpath value="f:Device/f:udiCarrier"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
