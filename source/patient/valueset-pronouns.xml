<?xml version="1.0" encoding="UTF-8"?>

<ValueSet xmlns="http://hl7.org/fhir">
  <id value="pronouns"/>
  <meta>
    <profile value="http://hl7.org/fhir/StructureDefinition/shareablevalueset"/>
  </meta>
  <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-wg">
    <valueCode value="pa"/>
  </extension>
  <url value="http://hl7.org/fhir/ValueSet/pronouns"/>
  <name value="Pronouns"/>
  <status value="draft"/>
  <publisher value="FHIR Project team"/>
  <contact>
    <telecom>
      <system value="url"/>
      <value value="http://hl7.org/fhir"/>
    </telecom>
  </contact>
  <description value="This example value set defines a set of codes that can be used to indicate the pronouns used to communicate about an individual."/>
  <compose>
    <include>
      <system value="http://hl7.org/fhir/pronouns"/>
	  <concept>
		<code value="he-him"/>
		<display value="He, Him, His, Himself"/>
	  </concept>
	  <concept>
		<code value="she-her"/>
		<display value="She, Her, Hers, Herself"/>
	  </concept>
	  <concept>
		<code value="they-them"/>
		<display value="They, Them, Their, Theirs, Themself"/>
	  </concept>
	  <concept>
		<code value="other"/>
		<display value="Other"/>
	  </concept>
	  <concept>
		<code value="unknown"/>
		<display value="Unknown"/>
	  </concept>
    </include>
  </compose>
</ValueSet>