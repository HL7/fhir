<?xml version="1.0" encoding="UTF-8"?>
<Patient xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://hl7.org/fhir ../../schema/patient.xsd">

  <extension>
    <url value="http://hl7.org/fhir/example-do-not-use#Patient.picture"/>
    <valueResource>
      <type value="Binary"/>
      <reference value="#pic1"/>
      <display value="Duck image"/>
    </valueResource>
  </extension>

  <text>
    <status value="generated"/>
    <div xmlns="http://www.w3.org/1999/xhtml">
      <p>Patient Donald DUCK @ Acme Healthcare, Inc. MR = 654321</p>
    </div>
  </text>
  
  <contained>
    <Binary id="pic1" contentType="image/gif"></Binary>
  </contained>

  <link>
    <type value="Patient"/>
    <reference value="patient/@pat2"/>
  </link>

  <active value="true"/>

  <identifier>
    <use value="usual"/>
    <label value="MRN"/>
    <system value="oid:0.1.2.3.4.5.6.7"/>
    <key value="654321"/>
  </identifier>
	
  <details>
    <name>
      <use value="official"/>
       <family value="Donald"/>
       <given value="Duck"/>
    </name>
    <gender>
      <system value="http://hl7.org/fhir/sid/v2-0001"/>
      <code value="M"/>
      <display value="Male"/>
    </gender>  
  </details>
  
  <provider>
    <type value="Organization"/>
    <reference value="organization/@1"/> 
    <display value="ACME Healthcare, Inc"/>
  </provider>
</Patient>