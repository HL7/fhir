        <code value="recipient"/>
        <type value="reference"/>
        <expression value="Invoice.recipient"/>
        <!--<xpath value="f:Invoice/f:recipient"/> -->
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
