      <constraint>
        <key value="msq-6"/>
        <severity value="error"/>
        <human value="Have and only have one of the following elements in referenceSeq : 1. referenceSequenceAssembly ; 2 referenceSequence"/>
        <expression value="(referenceSequenceAssembly.count()+referenceSequenceCodeableConcept.count()+ referenceSequenceReference.count()+ referenceSequenceString.count()) = 1"/>
        <xpath value="count(f:referenceSequenceAssembly)+count(f:referenceSequenceCodeableConcept)+count(f:referenceSequenceReference)+count(f:referenceSequenceString)=1"/>
        <source value="http://hl7.org/fhir/StructureDefinition/MolecularSequence"/>
      </constraint>
      <constraint>
        <key value="msq-5"/>
        <severity value="error"/>
        <human value="Both referenceSequenceAssembly and chromosome must be both contained if either one of them is contained"/>
        <expression value="(chromosome.empty() and referenceSequenceAssembly.empty()) or (chromosome.exists() and referenceSequenceAssembly.exists())"/>
        <xpath value="(exists(f:chromosome) and exists(f:referenceSequenceAssembly)) or (not(exists(f:chromosome)) and not(exists(f:referenceSequenceAssembly)))"/>
