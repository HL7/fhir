    <system value="url"/>
    <value value="http://hl7.org/fhir"/>
  </telecom>
  <description value="Example value set for Condition/Problem/Diagnosis codes"/>
  <status value="draft"/>
  <compose>
    <include>
      <system value="http://snomed.info/sct"/>
      <filter>
        <!-- any clinical finding -->
        <property value="concept"/>
