<?xml version="1.0" encoding="UTF-8"?>

<Bundle xmlns="http://hl7.org/fhir">
  <id value="EnrollmentResponse-search-params"/>
  <entry>
    <resource>
      <SearchParameter>
        <id value="EnrollmentResponse-identifier"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="EnrollmentResponse.identifier"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/EnrollmentResponse-identifier"/>
        <description value="The business identifier of the EnrollmentResponse"/>
        <code value="identifier"/>
        <type value="token"/>
        <expression value="EnrollmentResponse.identifier"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="EnrollmentResponse-request"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="EnrollmentResponse.request"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/EnrollmentResponse-request"/>
        <description value="The reference to the claim"/>
        <code value="request"/>
        <type value="reference"/>
        <expression value="EnrollmentResponse.request"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="EnrollmentResponse-status"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="EnrollmentResponse.status"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/EnrollmentResponse-status"/>
        <description value="The status of the enrollment response"/>
        <code value="status"/>
        <type value="token"/>
        <expression value="EnrollmentResponse.status"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
    <entry>
    <resource>
      <SearchParameter>
        <id value="EnrollmentResponse-patient"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="EnrollmentResponse.candidate"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/EnrollmentResponse-patient"/>
        <description value="The party to be enrolled"/>
        <code value="patient"/>
        <type value="reference"/>
        <expression value="EnrollmentResponse.candidate.where(resolve() is Patient)"/>
        <processingMode value="normal"/>
		<target value="Patient"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="EnrollmentResponse-group"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="EnrollmentResponse.candidate"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/EnrollmentResponse-group"/> 
        <description value="The parties to be enrolled"/>
        <code value="group"/>
        <type value="reference"/>
        <expression value="EnrollmentResponse.candidate.where(resolve() is Group)"/>
        <processingMode value="normal"/>
		<target value="Group"/>
      </SearchParameter>
    </resource>
  </entry>

</Bundle>