<ValueSet xmlns="http://hl7.org/fhir" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://hl7.org/fhir ../../schema/valueset.xsd">
  <name value="SpecimenType"/>
  <publisher value="FHIR Project team"/>
  <telecom>  
    <system value="url"/>
    <value value="http://hl7.org/fhir"/>
  </telecom>
  <description value=" This example value set defines a set of codes that can be used to indicate the type of specimen."/>
  <status value="draft"/>
  <define>
    <system value="http://hl7.org/fhir/specimen-type"/>
    <concept>
      <code value="food"/>
      <display value="Food"/>
      <definition value="Specimen collected from a substance which serves as sustenance"/>
    </concept>
    <concept>
      <code value="human"/>
      <display value="Human"/>
      <definition value="Specimen collected from a human"/>
    </concept>
    <concept>
      <code value="animal"/>
      <display value="Animal"/>
      <definition value="Specimen collected from an animal"/>
    </concept>
    <concept>
      <code value="environmental"/>
      <display value="Environmental"/>
      <definition value="Specimen collected from the environment. For instance, a water sample from a lake."/>
    </concept>
  </define>
</ValueSet>

