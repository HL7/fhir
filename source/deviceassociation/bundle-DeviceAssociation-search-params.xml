<?xml version="1.0" encoding="UTF-8"?>

<Bundle xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://hl7.org/fhir ../../publish/Bundle.xsd">
  <id value="DeviceAssociation-search-params"/>
    <entry>
    <resource>
      <SearchParameter>
        <id value="DeviceAssociation-code"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="DeviceAssociation.device"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/DeviceAssociation-code"/>
        <description value="Search for products that match this code"/>
        <code value="device"/>
        <type value="reference"/>
        <expression value="DeviceAssociation.device"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
   <entry>
    <resource>
      <SearchParameter>
        <id value="DeviceAssociation-patient"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="DeviceAssociation.subject"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/DeviceAssociation-patient"/>
        <description value="Search for patient for whom to list associations"/>
        <code value="patient"/>
        <type value="reference"/>
        <expression value="DeviceAssociation.subject.where(resolve() is Patient)"/>
        <processingMode value="normal"/>
        <target value="Patient"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="DeviceAssociation-operation-period"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="DeviceAssociation.operation.period"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/DeviceAssociation-operation-period"/>
        <description value="Search the device's operation during a period of time"/>
        <code value="operation-period"/>
        <type value="date"/>
        <expression value="DeviceAssociation.operation.period"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="DeviceAssociation-period"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="DeviceAssociation.period"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/DeviceAssociation-period"/>
        <description value="Search association during a period of time"/>
        <code value="period"/>
        <type value="date"/>
        <expression value="DeviceAssociation.period"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
    <entry>
    <resource>
      <SearchParameter>
        <id value="DeviceAssociation-status"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="DeviceAssociation.status"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/DeviceAssociation-status"/>
        <description value="Search based on the status of the association,	implanted | explanted | attached | entered-in-error | unknown"/>
        <code value="status"/>
        <type value="token"/>
        <expression value="DeviceAssociation.status"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>  
  <entry>
    <resource>
      <SearchParameter>
        <id value="DeviceAssociation-subject"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="DeviceAssociation.subject"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/DeviceAssociation-subject"/>
        <description value="Search for by all subjects to list associations"/>
        <code value="subject"/>
        <type value="reference"/>
        <expression value="DeviceAssociation.subject"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="DeviceAssociation-operator"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="DeviceAssociation.operation.operator"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/DeviceAssociation-operator"/>
        <description value="Search for the individual operating the device during an association"/>
        <code value="operator"/>
        <type value="reference"/>
        <expression value="DeviceAssociation.operation.operator"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>  

</Bundle>