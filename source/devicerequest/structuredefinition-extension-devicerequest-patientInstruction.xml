  <description value="Simple concise instructions to be read by the patient.  For example  “twice a day” rather than “BID.”."/>
  <fhirVersion value="5.0.0"/>
  <mapping>
    <identity value="rim"/>
    <uri value="http://hl7.org/v3"/>
