<ValueSet xmlns="http://hl7.org/fhir" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://hl7.org/fhir ../../schema/valueset.xsd">
 <name value="PractitionerRole"/>
 
  <publisher value="FHIR Project team"/>
  <telecom>  
    <system value="url"/>
    <value value="http://hl7.org/fhir"/>
  </telecom>
  <description value="This example value set defines a set of codes that can be used to indicate the role of a Practitioner."/>
  <status value="draft"/>
  <define>
    <system value="http://hl7.org/fhir/practitioner-role"/>
    <concept>
      <code value="doctor"/>
    </concept>
    <concept>
      <code value="nurse"/>
    </concept>
    <concept>
      <code value="pharmacist"/>
    </concept>
    <concept>
      <code value="researcher"/>
    </concept>    
    <concept>
      <code value="teacher"/>
      <display value="Teacher/educator" />
    </concept>
    <concept>
      <code value="ict"/>
      <display value="ICT professional" />
    </concept>        
  </define>
</ValueSet>

