        <code value="Range"/>
      </type>
      <type>
        <code value="string"/>
      </type>
      <condition value="fhs-2"/>
      <condition value="fhs-1"/>
      <condition value="fhs-3"/>
      <isSummary value="true"/>
      <mapping>
