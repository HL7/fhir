<ValueSet xmlns="http://hl7.org/fhir" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://hl7.org/fhir ../../schema/valueset.xsd">
  <name value="QuestionnaireName"/>
  <publisher value="FHIR Project team"/>
  <telecom>  
    <system value="url"/>
    <value value="http://hl7.org/fhir"/>
  </telecom>
  <description value="This example value set defines a set of codes that can be used to specify a coded name for a standard Questionnaire."/>
  <status value="draft"/>
  <define>
    <system value="http://hl7.org/fhir/questionnaire-name"/>
    <caseSensitive value="false"/>
    <concept>
      <code value="CTQQ-2012"/>
      <display value="Cancer Treatment Quality Questionnaire 2012"/>
    </concept>
    <concept>
      <code value="PA.form.A"/>
      <display value="Pre-admission standard Form A"/>
    </concept>
    <concept>
      <code value="BNAS"/>
      <display value="Brazelton Neonatal Assessment Scale"/>
    </concept>
  </define>
</ValueSet>

