	<description value="The criteria by which a question is enabled."/>
	<caseSensitive value="true"/>
	<valueSet value="http://hl7.org/fhir/ValueSet/questionnaire-enable-operator"/>
	<content value="complete"/>
	<concept>
		<code value="exists"/>
		<display value="Exists"/>
		<definition value="True if the determination of 'whether an answer exists for the question' is equal to the enableWhen answer (which must be a boolean)."/>
	</concept>
	<concept>
		<code value="="/>
		<display value="Equals"/>
		<definition value="True if at least one answer has a value that is equal to the enableWhen answer."/>
	</concept>
	<concept>
		<code value="!="/>
		<display value="Not Equals"/>
		<definition value="True if no answer has a value that is equal to the enableWhen answer."/>
	</concept>
	<concept>
		<code value="&gt;"/>
		<display value="Greater Than"/>
		<definition value="True if at least one answer has a value that is greater than the enableWhen answer."/>
	</concept>
	<concept>
		<code value="&lt;"/>
		<display value="Less Than"/>
		<definition value="True if at least one answer has a value that is less than the enableWhen answer."/>
	</concept>
	<concept>
		<code value="&gt;="/>
		<display value="Greater or Equals"/>
		<definition value="True if at least one answer has a value that is greater or equal to the enableWhen answer."/>
	</concept>
	<concept>
		<code value="&lt;="/>
		<display value="Less or Equals"/>
		<definition value="True if at least one answer has a value that is less or equal to the enableWhen answer."/>
	</concept>
