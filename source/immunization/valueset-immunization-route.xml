      </concept>
      <concept>
        <code value="IM"/>
        <display value="Injection, intramuscular"/>
      </concept>
      <concept>
        <code value="IVINJ"/>
        <display value="Injection, intravenous"/>
      </concept>
      <concept>
        <code value="PO"/>
        <display value="Swallow, oral"/>
      </concept>
      <concept>
        <code value="SQ"/>
        <display value="Injection, subcutaneous"/>
      </concept>
      <concept>
        <code value="TRNSDERM"/>
        <display value="Transdermal"/>
      </concept>
    </include>
  </compose>
</ValueSet>