  <url value="http://hl7.org/fhir/ValueSet/immunization-route"/>
  <identifier>
    <system value="urn:ietf:rfc:3986"/>
    <value value="urn:oid:2.16.840.1.113883.4.642.3.289"/>
  </identifier>
  <version value="5.0.0"/>
  <name value="ImmunizationRouteCodes"/>
