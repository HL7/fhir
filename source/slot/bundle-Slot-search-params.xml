          <valueString value="Slot.schedule"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Slot-schedule"/>
        <description value="The Schedule Resource that we are seeking a slot within"/>
        <code value="schedule"/>
        <type value="reference"/>
        <expression value="Slot.schedule"/>
        <xpath value="f:Slot/f:schedule"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
