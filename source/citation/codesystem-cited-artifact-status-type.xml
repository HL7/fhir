  <concept>
    <code value="created"/>
    <display value="Created"/>
    <definition value="The content was originally constructed or composed."/>
  </concept>
  <concept>
    <code value="submitted"/>
    <display value="Submitted"/>
    <definition value="The content was sent to the publisher for consideration of publication."/>
  </concept>
  <concept>
    <code value="withdrawn"/>
    <display value="Withdrawn"/>
    <definition value="The content that was not published has been removed from consideration for publishing by the submitter."/>
  </concept>
  <concept>
    <code value="pre-review"/>
    <display value="Pre review"/>
    <definition value="The content is awaiting assignment and delivery to reviewer(s)."/>
  </concept>
  <concept>
    <code value="under-review"/>
    <display value="Under review"/>
    <definition value="The content is in a state of being reviewed."/>
  </concept>
  <concept>
    <code value="post-review-pre-published"/>
    <display value="Post review pre published"/>
    <definition value="The content is in a state between the review(s) being completed and being published."/>
  </concept>
  <concept>
    <code value="rejected"/>
    <display value="Rejected"/>
    <definition value="The content that was not published has been removed from consideration for publishing by a publisher or editor."/>
  </concept>
  <concept>
    <code value="published-early-form"/>
    <display value="Published early form"/>
    <definition value="The content is published but future changes to the published version are expected."/>
  </concept>
  <concept>
    <code value="published-final-form"/>
    <display value="Published final form"/>
    <definition value="The content is published and further changes to the content are not expected."/>
  </concept>
  <concept>
    <code value="accepted"/>
    <display value="Accepted"/>
    <definition value="The content that was not published yet has been approved for publication by the publisher and/or editor."/>
  </concept>
  <concept>
    <code value="archived"/>
    <display value="Archived"/>
    <definition value="The content is retired or considered no longer current but still available as part of the public record."/>
  </concept>
  <concept>
    <code value="retracted"/>
    <display value="Retracted"/>
    <definition value="The content that was published is removed from publication and should no longer be considered part of the public record."/>
  </concept>
  <concept>
    <code value="draft"/>
    <display value="Draft"/>
    <definition value="The content is considered unfinished or incomplete and not representative of the current state desired by the content creator."/>
  </concept>
  <concept>
    <code value="active"/>
    <display value="Active"/>
    <definition value="The content is considered complete for its current state by the content creator."/>
  </concept>
  <concept>
    <code value="approved"/>
    <display value="Approved"/>
    <definition value="The content has been approved for a state transition, with the focus of approval described in the text associated with this coding."/>
  </concept>
  <concept>
    <code value="unknown"/>
    <display value="Unknown"/>
    <definition value="The status of the content is not recorded in the metadata."/>
  </concept>
