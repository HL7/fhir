<?xml version="1.0" encoding="UTF-8"?>

<Bundle xmlns="http://hl7.org/fhir">
  <id value="Evidence-search-params"/>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Evidence-context"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="{{name}}.useContext.valueCodeableConcept"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Evidence-context"/>
        <description value="A use context assigned to the evidence"/>
        <code value="context"/>
        <type value="token"/>
        <expression value="(Evidence.useContext.value as CodeableConcept)"/>
        <xpath value="f:Evidence/f:useContext/f:valueCodeableConcept"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Evidence-context-quantity"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="{{name}}.useContext.valueQuantity,{{name}}.useContext.valueRange"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Evidence-context-quantity"/>
        <description value="A quantity- or range-valued use context assigned to the evidence"/>
        <code value="context-quantity"/>
        <type value="quantity"/>
        <expression value="(Evidence.useContext.value as Quantity) | (Evidence.useContext.value as Range)"/>
        <xpath value="f:Evidence/f:useContext/f:valueQuantity | f:Evidence/f:useContext/f:valueRange"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Evidence-context-type"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="{{name}}.useContext.code"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Evidence-context-type"/>
        <description value="A type of use context assigned to the evidence"/>
        <code value="context-type"/>
        <type value="token"/>
        <expression value="Evidence.useContext.code"/>
        <xpath value="f:Evidence/f:useContext/f:code"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Evidence-context-type-quantity"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Evidence-context-type-quantity"/>
        <description value="A use context type and quantity- or range-based value assigned to the evidence"/>
        <code value="context-type-quantity"/>
        <type value="composite"/>
        <expression value="Evidence.useContext"/>
        <xpathUsage value="normal"/>
        <component>
          <definition value="context-type"/>
          <expression value="code"/>
        </component>
        <component>
          <definition value="context-quantity"/>
          <expression value="value.as(Quantity) | value.as(Range)"/>
        </component>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Evidence-context-type-value"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Evidence-context-type-value"/>
        <description value="A use context type and value assigned to the evidence"/>
        <code value="context-type-value"/>
        <type value="composite"/>
        <expression value="Evidence.useContext"/>
        <xpathUsage value="normal"/>
        <component>
          <definition value="context-type"/>
          <expression value="code"/>
        </component>
        <component>
          <definition value="context"/>
          <expression value="value.as(CodeableConcept)"/>
        </component>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Evidence-date"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="normative"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="{{name}}.date"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Evidence-date"/>
        <description value="The evidence publication date"/>
        <code value="date"/>
        <type value="date"/>
        <expression value="Evidence.date"/>
        <xpath value="f:Evidence/f:date"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Evidence-description"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="normative"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="{{name}}.description"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Evidence-description"/>
        <description value="The description of the evidence"/>
        <code value="description"/>
        <type value="string"/>
        <expression value="Evidence.description"/>
        <xpath value="f:Evidence/f:description"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Evidence-identifier"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="{{name}}.identifier"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Evidence-identifier"/>
        <description value="External identifier for the evidence"/>
        <code value="identifier"/>
        <type value="token"/>
        <expression value="Evidence.identifier"/>
        <xpath value="f:Evidence/f:identifier"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Evidence-publisher"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="normative"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="{{name}}.publisher"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Evidence-publisher"/>
        <description value="Name of the publisher of the evidence"/>
        <code value="publisher"/>
        <type value="string"/>
        <expression value="Evidence.publisher"/>
        <xpath value="f:Evidence/f:publisher"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Evidence-status"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="normative"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="{{name}}.status"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Evidence-status"/>
        <description value="The current status of the evidence"/>
        <code value="status"/>
        <type value="token"/>
        <expression value="Evidence.status"/>
        <xpath value="f:Evidence/f:status"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Evidence-title"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="normative"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="{{name}}.title"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Evidence-title"/>
        <description value="The human-friendly name of the evidence"/>
        <code value="title"/>
        <type value="string"/>
        <expression value="Evidence.title"/>
        <xpath value="f:Evidence/f:title"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Evidence-url"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="normative"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="{{name}}.url"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Evidence-url"/>
        <description value="The uri that identifies the evidence"/>
        <code value="url"/>
        <type value="uri"/>
        <expression value="Evidence.url"/>
        <xpath value="f:Evidence/f:url"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Evidence-version"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="normative"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="{{name}}.version"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Evidence-version"/>
        <description value="The business version of the evidence"/>
        <code value="version"/>
        <type value="token"/>
        <expression value="Evidence.version"/>
        <xpath value="f:Evidence/f:version"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
</Bundle>