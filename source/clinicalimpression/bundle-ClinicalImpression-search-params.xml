          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="ClinicalImpression.problem"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/ClinicalImpression-problem"/>
        <description value="Relevant impressions of patient state"/>
        <code value="problem"/>
        <type value="reference"/>
        <expression value="ClinicalImpression.problem"/>
        <!--<xpath value="f:ClinicalImpression/f:problem"/> -->
