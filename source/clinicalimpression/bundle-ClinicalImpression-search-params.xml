        <description value="Reference to last assessment"/>
        <code value="previous"/>
        <type value="reference"/>
        <expression value="ClinicalImpression.previous"/>
        <!--<xpath value="f:ClinicalImpression/f:previous"/> -->
        <processingMode value="normal"/>
