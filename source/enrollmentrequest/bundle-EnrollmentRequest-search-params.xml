<?xml version="1.0" encoding="UTF-8"?>

<Bundle xmlns="http://hl7.org/fhir">
  <id value="EnrollmentRequest-search-params"/>
  <entry>
    <resource>
      <SearchParameter>
        <id value="EnrollmentRequest-identifier"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="EnrollmentRequest.identifier"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/EnrollmentRequest-identifier"/>
        <description value="The business identifier of the Enrollment"/>
        <code value="identifier"/>
        <type value="token"/>
        <expression value="EnrollmentRequest.identifier"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="EnrollmentRequest-patient"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="EnrollmentRequest.candidate"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/EnrollmentRequest-patient"/>
        <description value="The party to be enrolled"/>
        <code value="patient"/>
        <type value="reference"/>
        <expression value="EnrollmentRequest.candidate.where(resolve() is Patient)"/>
        <processingMode value="normal"/>
		<target value="Patient"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="EnrollmentRequest-group"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="EnrollmentRequest.candidate"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/EnrollmentRequest-group"/>
        <description value="The parties to be enrolled"/>
        <code value="group"/>
        <type value="reference"/>
        <expression value="EnrollmentRequest.candidate.where(resolve() is Group)"/>
        <processingMode value="normal"/>
		<target value="Group"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="EnrollmentRequest-status"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status"> 
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="EnrollmentRequest.status"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/EnrollmentRequest-status"/>
        <description value="The status of the enrollment"/>
        <code value="status"/>
        <type value="token"/>
        <expression value="EnrollmentRequest.status"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="EnrollmentRequest-subject"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="EnrollmentRequest.candidate"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/EnrollmentRequest-subject"/>
        <description value="The party to be enrolled"/>
        <code value="subject"/>
        <type value="reference"/>
        <expression value="EnrollmentRequest.candidate"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
</Bundle>