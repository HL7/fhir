        <type value="token"/>
        <expression value="AdministrableProductDefinition.routeOfAdministration.targetSpecies.code"/>
        <xpath value="f:AdministrableProductDefinition/f:routeOfAdministration/f:targetSpecies/f:code"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
</Bundle>