          <valueString value="EpisodeOfCare.diagnosis.condition.reference"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/EpisodeOfCare-condition"/>
        <description value="Conditions/problems/diagnoses this episode of care is for (legacy)"/>
        <comment value="(retained for backward compatibility)"/>
        <code value="condition"/>
        <type value="reference"/>
        <expression value="EpisodeOfCare.diagnosis.condition.reference"/>
        <xpath value="f:EpisodeOfCare/f:diagnosis/f:condition/f:reference"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
