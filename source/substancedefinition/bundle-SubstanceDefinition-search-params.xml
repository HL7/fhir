<?xml version="1.0" encoding="UTF-8"?>

<Bundle xmlns="http://hl7.org/fhir">
  <id value="SubstanceDefinition-search-params"/>
  <entry>
    <resource>
      <SearchParameter>
        <id value="SubstanceDefinition-category"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="SubstanceDefinition.category"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/SubstanceDefinition-category"/>
        <description value="High level categorization, e.g. polymer or nucleic acid, or food, chemical, biological"/>
        <code value="category"/>
        <type value="token"/>
        <expression value="SubstanceDefinition.category"/>
        <xpath value="f:SubstanceDefinition/f:category"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
