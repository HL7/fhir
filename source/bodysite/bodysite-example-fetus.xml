<?xml version="1.0" encoding="UTF-8"?>
<!--This example demonstrates using the bodysite resource to identify of fetus within systems that choose not to treat a fetus as a Patient-->
<BodySite xmlns="http://hl7.org/fhir" xmlns:xhtml="http://www.w3.org/1999/xhtml">
	<id value="fetus"/>
	<identifier>
		<system value="http://goodhealth.org/bodysite/identifiers"/>
		<value value="12345"/>
	</identifier>
	<code>
		<coding>
			<system value="http://snomed.info/sct"/>
			<code value="83418008"/>
			<display value="Entire fetus (body structure)"/>
		</coding>
		<text value="Fetus"/>
	</code>
	<description value="EDD 1/1/2017 confirmation by LMP"/>
	<patient>
		<reference value="Patient/example"/>
	</patient>
</BodySite>
