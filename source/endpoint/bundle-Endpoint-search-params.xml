<?xml version="1.0" encoding="UTF-8"?>

<Bundle xmlns="http://hl7.org/fhir">
  <id value="Endpoint-search-params"/>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Endpoint-connection-type"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="Endpoint.connectionType"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Endpoint-connection-type"/>
        <description value="Protocol/Profile/Standard to be used with this endpoint connection"/>
        <code value="connection-type"/>
        <type value="token"/>
        <expression value="Endpoint.connectionType"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Endpoint-identifier"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="Endpoint.identifier"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Endpoint-identifier"/>
        <description value="Identifies this endpoint across multiple systems"/>
        <code value="identifier"/>
        <type value="token"/>
        <expression value="Endpoint.identifier"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Endpoint-name"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="Endpoint.name"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Endpoint-name"/>
        <description value="A name that this endpoint can be identified by"/>
        <code value="name"/>
        <type value="string"/>
        <expression value="Endpoint.name"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Endpoint-organization"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="Endpoint.managingOrganization"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Endpoint-organization"/>
        <description value="The organization that is managing the endpoint"/>
        <code value="organization"/>
        <type value="reference"/>
        <expression value="Endpoint.managingOrganization"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Endpoint-payload-type"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="Endpoint.payload.type"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Endpoint-payload-type"/>
        <description value="The type of content that may be used at this endpoint (e.g. XDS Discharge summaries)"/>
        <code value="payload-type"/>
        <type value="token"/>
        <expression value="Endpoint.payload.type"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Endpoint-payload-profile"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="Endpoint.payload.profileCanonical,Endpoint.payload.profileUri"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Endpoint-payload-profile"/>
        <description value="The profiles (FHIR or non-FHIR) that might be used at this endpoint"/>
        <code value="payload-profile"/>
        <type value="reference"/>
        <expression value="Endpoint.payload.profileCanonical | Endpoint.payload.profileUri"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Endpoint-status"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="Endpoint.status"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Endpoint-status"/>
        <description value="The current status of the Endpoint (usually expected to be active)"/>
        <code value="status"/>
        <type value="token"/>
        <expression value="Endpoint.status"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
</Bundle>