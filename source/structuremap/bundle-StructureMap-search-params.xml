<?xml version="1.0" encoding="UTF-8"?>

<Bundle xmlns="http://hl7.org/fhir">
  <id value="StructureMap-search-params"/>
  <entry>
    <resource>
      <SearchParameter>
        <id value="StructureMap-context"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="{{name}}.useContext.valueCodeableConcept"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/StructureMap-context"/>
        <description value="A use context assigned to the structure map"/>
        <code value="context"/>
        <type value="token"/>
        <expression value="(StructureMap.useContext.value.ofType(CodeableConcept))"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="StructureMap-context-quantity"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="{{name}}.useContext.valueQuantity,{{name}}.useContext.valueRange"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/StructureMap-context-quantity"/>
        <description value="A quantity- or range-valued use context assigned to the structure map"/>
        <code value="context-quantity"/>
        <type value="quantity"/>
        <expression value="(StructureMap.useContext.value.ofType(Quantity)) | (StructureMap.useContext.value.ofType(Range))"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="StructureMap-context-type"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="{{name}}.useContext.code"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/StructureMap-context-type"/>
        <description value="A type of use context assigned to the structure map"/>
        <code value="context-type"/>
        <type value="token"/>
        <expression value="StructureMap.useContext.code"/>
        <processingMode value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="StructureMap-context-type-quantity"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/StructureMap-context-type-quantity"/>
        <description value="A use context type and quantity- or range-based value assigned to the structure map"/>
        <code value="context-type-quantity"/>
        <type value="composite"/>
        <expression value="StructureMap.useContext"/>
        <processingMode value="normal"/>
        <component>
          <definition value="context-type"/>
          <expression value="code"/>
        </component>
        <component>
          <definition value="context-quantity"/>
          <expression value="value.ofType(Quantity) | value.ofType(Range)"/>
        </component>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="StructureMap-context-type-value"/>
